        <!-- Navbar Start -->
        <nav style="width:100%;" class="navbar navbar-expand-lg bg-white navbar-light sticky-top px-4 px-lg-5">
            <a href="index.html" class="navbar-brand d-flex align-items-center">
                <h1 class="m-0">
                    <img class="img-fluid me-3"
                         src="~/img/icon/icon-02-primary.png"
                         alt="" />Insure
                </h1>
            </a>
            <button type="button"
                    class="navbar-toggler"
                    data-bs-toggle="collapse"
                    data-bs-target="#navbarCollapse">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarCollapse">
                <div class="navbar-nav mx-auto bg-light rounded pe-4 py-3 py-lg-0">
                    <a href="@Url.Action("Home","Users")" class="nav-item nav-link active">Home</a>
                    <a href="@Url.Action("about","Users")" class="nav-item nav-link">About Us</a>
                    <a href="@Url.Action("Service","Users")" class="nav-item nav-link">Incurance Plans</a>


                    <div class="nav-item dropdown">
                        <a href="#"
                           class="nav-link dropdown-toggle"
                           data-bs-toggle="dropdown">Buy Online</a>
                        <div class="dropdown-menu bg-light border-0 m-0">
                            <a href="@Url.Action("Create","Lifeinsurances")" class="dropdown-item">Life Insurance</a>
                            <a href="@Url.Action("Create","Healthinsurances")" class="dropdown-item">Health Insurance</a>
                            <a href="@Url.Action("Create","Homeinsurances")" class="dropdown-item">Home Insurance</a>
                            <a href="@Url.Action("Create","Motorinsurances")" class="dropdown-item">Motor Insurance</a>

